{
  "kind": "collectionType",
  "collectionName": "order_billings",
  "info": {
    "singularName": "order-billing",
    "pluralName": "order-billings",
    "displayName": "Order Billing"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 20
    },
    "lastname": {
      "type": "string",
      "required": true,
      "maxLength": 20
    },
    "address": {
      "type": "text",
      "required": true,
      "maxLength": 200
    },
    "phone": {
      "type": "integer",
      "required": true,
      "max": 20
    },
    "email": {
      "type": "email",
      "required": true,
      "maxLength": 100
    },
    "order": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order.order",
      "inversedBy": "billing"
    }
  }
}
